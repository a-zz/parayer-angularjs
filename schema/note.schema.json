{ 
	"$schema": "http://json-schema.org/schema#",
	"$id": "https://pareidolia.es/schemas/parayer/0.0/note.schema.json",
	"title": "Db object type Note",
	"type": "object",
	"properties": {
		"_id": {
			"type": "string",
			"pattern": "[a-f0-9]{8}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{12}"
		},
		"_rev": {
			"type": "string",
			"pattern": "[0-9]+-[a-f0-9]{32}"
		},
		"summary": {
			"type": "string",
			"minLength": 1
		},
		"descr": {
			"type": "string"
		},
		"attachedTo": {
			"type": "string",
			"pattern": "[a-f0-9]{8}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{12}"
		},
		"usr": {
			"type": "string",
			"pattern": "[a-f0-9]{8}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{12}"
		},
		"date": {
			"type": "string",
			"pattern": "[0-9]{4}-((0[1-9]{1})|(1[1-2]{1}))-((0[1-9]{1})|(1[0-9]{1})|(2[0-9]{1})|(3[0-1]{1})) ((0[0-9]{1})|(1[0-9]{1})|(2[0-3]{1})):[0-5]{1}[0-9]{1}:[0-5]{1}[0-9]{1}"
		}
	}
}
